{
    "name": "cyber-duck/model-files",
    "description": "Model HasFiles Trait for Eloquent Models",
    "authors": [
        {
            "name": "Tiago Tavares",
            "email": "tiago@cyber-duck.co.uk"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.1",
        "illuminate/http": "~5.1"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Cyberduck\\ModelFiles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cyberduck\\ModelFiles\\ModelFilesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
